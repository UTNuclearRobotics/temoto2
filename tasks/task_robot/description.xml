<?xml version="1.0" ?>
<task action="start" alias="load,make,set">

  <!--Interface block-->
  <interface id="0" type="asynchronous">
    <!--Input block-->
    <in>    
      <what>
        <arg word="robot"/>
      </what>
    </in>
  </interface>

  <!--Interface block-->
  <interface id="1" type="synchronous">
    <!--Input block-->
    <in>    
      <what>
        <arg word="plan"/>
      </what>
    </in>
  </interface>

  <!--Interface block-->
  <interface id="2" type="synchronous">
    <!--Input block-->
    <in>    
      <what>
        <arg word="executing"/>
      </what>
    </in>
  </interface>

  <!--Interface block-->
  <interface id="3" type="synchronous">
    <!--Input block-->
    <in>    
      <what>
        <arg word="target"/>
      </what>
    </in>
  </interface>

</task>
