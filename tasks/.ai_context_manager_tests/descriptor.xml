<?xml version="1.0" ?>

<task action="start" alias="initiate,track">

  <!--Interface block-->
  <interface id="0" type="synchronous">
    
    <!--Input block-->
    <in>    
      <what>
        <arg word="motor,engine,bird"/>
      </what>
    </in>
   
  </interface>


  <!--Interface block-->
  <interface id="1" type="asynchronous">
    
    <!--Input block-->
    <in>    
      <what>
        <arg word="camera"/>
      </what>
    </in>
    
    <!--Output block-->
    <out>
      <what>
        <arg word="camera"/>
        <data>
          <field datatype="topic"/>
        </data>
      </what> 
    </out>

  </interface>

  <!--Interface block-->
  <interface id="2" type="asynchronous">
    
    <!--Input block-->
    <in>    
      <what>
        <arg word="hand,hand tracker"/>
      </what>
    </in>
    
    <!--Output block-->
    <out>
      <what>
        <arg word="hand,hand tracker"/>
        <data>
          <field datatype="topic"/>
        </data>
      </what> 
    </out>

  </interface>

</task>
